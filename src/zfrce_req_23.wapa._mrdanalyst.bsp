  create object lr_field  exporting ir_page = page.                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
  ir_layout_service->SET_ROW( IV_ROW = 0 ).                                                                                                                                                                                                                    
  ir_layout_service->SET_COLUMN( IV_COLUMN = 1 ).                                                                                                                                                                                                              
%>                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
