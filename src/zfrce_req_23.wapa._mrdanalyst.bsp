     <htmlb:groupBody>                                                                                                                                                                                                                                         
        <htmlb:gridLayout columnSize="2" rowSize="17" width="100%">                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
  <%-- ----------------------When will this delivered------------------------------ --%>                                                                                                                                                                       
                                                                                                                                                                                                                                                               
  <%                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
  if controller->mv_display_only eq 'TRUE'.                                                                                                                                                                                                                    
    l_display_only = 'TRUE'.                                                                                                                                                                                                                                   
  else.                                                                                                                                                                                                                                                        
    l_display_only = 'FALSE'.                                                                                                                                                                                                                                  
  endif.                                                                                                                                                                                                                                                       
           ir_layout_service->set_next_row( ).                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
           l_prefix = controller->get_prefix_for_structure( 'MS_FRCE' ).                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
           lr_field->set_field( iv_name         = 'FRC_WHNDELIVERY'                                                                                                                                                                                            
