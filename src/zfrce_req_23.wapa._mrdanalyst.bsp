                                iv_prefix       = l_prefix                                                                                                                                                                                                     
                                iv_label        = 'Impacts to system, processes & people'                                                                                                                                                                      
                                iv_disabled     = l_display_only ).                                                                                                                                                                                            
        l_tooltip = '/DS1/ZFRC_IMP_SYS'.                                                                                                                                                                                                                       
%>                                                                                                                                                                                                                                                             
     <%@ include file = "cell_with_justification_inputfield.bsp" %>                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
