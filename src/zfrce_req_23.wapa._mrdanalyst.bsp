                                iv_prefix       = l_prefix                                                                                                                                                                                                     
                                iv_label        = 'Cost & Time'                                                                                                                                                                                                
                                iv_disabled     = l_display_only ).                                                                                                                                                                                            
        l_tooltip = '/DS1/ZFRC_ESTCOST'.                                                                                                                                                                                                                       
%>                                                                                                                                                                                                                                                             
     <%@ include file = "cell_with_justification_inputfield.bsp" %>                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
</htmlb:gridLayout>                                                                                                                                                                                                                                            
 </htmlb:groupBody>                                                                                                                                                                                                                                            
</htmlb:group>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               