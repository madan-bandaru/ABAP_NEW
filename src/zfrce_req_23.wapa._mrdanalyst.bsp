<%@extension name="/ds1/fss" prefix="mdm" %>                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
<%                                                                                                                                                                                                                                                             
   data:                                                                                                                                                                                                                                                       
     l_rowsize_view  type string,       "Number of rows in view                                                                                                                                                                                                
     l_display_only  type string,                                                                                                                                                                                                                              
